    <artifactId>percentagecolumn</artifactId>
    <packaging>hpi</packaging>
    <version>0.0.1-SNAPSHOT</version>
    <name>Percentage Disk Space Column plugin</name>
    <url>http://wiki.jenkins-ci.org/display/JENKINS/PercentageColumn+Plugin</url>

    <description>
        This plugin just shows the percentage of disk space usage column on "Manage Nodes" page.
    </description>
    <issueManagement>
        <system>JIRA</system>
        <url>https://issues.jenkins-ci.org/secure/IssueNavigator.jspa?reset=true&amp;jqlQuery=project+%3D+JENKINS+AND+status+in+%28Open%2C+%22In+Progress%22%2C+Reopened%29+AND+component+%3D+%27percentagecolumn-plugin%27</url>
    </issueManagement>

    <licenses>
        <license>
            <name>The MIT license</name>
            <url>https://github.com/jenkinsci/percentagecolumn-plugin/raw/master/LICENSE.txt</url>
            <distribution>repo</distribution>
        </license>
    </licenses>

    <distributionManagement>
        <repository>
            <id>maven.jenkins-ci.org</id>
            <url>http://maven.jenkins-ci.org:8081/content/repositories/releases/</url>
        </repository>
    </distributionManagement>

    <developers>
      <developer>
        <id>v1v</id>
        <name>Victor Martinez</name>
        <email>VictorMartinezRubio@gmail.com</email>
      </developer>
    </developers>

    <scm>
        <connection>scm:git:git://github.com/jenkinsci/percentagecolumn-plugin.git</connection>
        <developerConnection>scm:git:git@github.com:jenkinsci/percentagecolumn-plugin.git</developerConnection>
        <url>http://github.com/jenkinsci/percentagecolumn-plugin</url>
    </scm>

    <properties>
